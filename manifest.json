{
    "name": "Speedy",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "all_frames": true
        }
    ],
    "browser_action" : {
        "default_popup": "popup.html",
        "default_title": "Speedy"
    },
    "permissions" : [
        "identity"
    ]
}